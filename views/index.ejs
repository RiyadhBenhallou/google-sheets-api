<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <table>
      <thead>
        <tr>
          <% rows[0].forEach(column => { %>
          <th><%= column %></th>
          <% }) %>
        </tr>
      </thead>
      <tbody>
        <% rows.slice(1).forEach(row => { %>
        <tr>
          <td><%= row[0] %></td>
          <td><%= row[1] %></td>
          <td><%= row[2] %></td>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
